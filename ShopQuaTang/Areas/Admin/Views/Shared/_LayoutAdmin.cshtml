    <!-- # END Top Bar -->

    <section class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </section>

    <!-- Jquery Core Js -->
    <script src="~/Assets/Admin/plugins/jquery/jquery.min.js"></script>

